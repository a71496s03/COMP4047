package hk.edu.hkbu.comp;


public class Main {
	public static void main(String[] args){
		MyDatabase db = new MyDatabase("test.txt");
		db.newWebsite("www.111.com");
		db.addTitle("Java Create and Write To Files");
		db.addContent("To create a file in Java, you can use the createNewFile() method. This method returns a boolean value: true if the file was successfully created, and false if the file already exists. Note that the method is enclosed in a try...catch block. This is necessary because it throws an IOException if an error occurs (if the file cannot be created for some reason):");
		db.save();
		db.save();
		//db.web.printJsonObject(db.web.getJSON());
	}
}