public class MyWebsite {
	public String url;
	JSONObject title = new JSONObject();
	JSONObject content = new JSONObject();
	
	public MyWebsite(String url) {
		this.url=url;
	}
	
	public void addTitle(String word, int posotion) {
		JSONArray tmp = new JSONArray();
		if (title.containsKey(word))
			tmp = title.getJSONArray(word);
		tmp.add(posotion);
	}
	
	public void addContent(String word, int posotion) {
		
	}
}